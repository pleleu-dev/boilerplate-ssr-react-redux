		"webpack-merge": "4.1.4",
		"webpack-node-externals": "^1.7.2"
	},
	"scripts": {
		"dev:build-client": "webpack --env devClient --watch",
		"dev:build-server": "webpack --env devServer --watch",
		"dev:start": "nodemon --exec node --inspect ./build/index.js",
		"dev": "npm-run-all --parallel dev:*",
		"prod:build-client": "webpack --env prodClient",
		"prod:build-server": "webpack --env prodServer",
		"prod": "npm-run-all --parallel prod:*"
	},
	"nodemonConfig": {
		"ext": "*.js",
		"ignore": [
			"config/*",
			"node_modules/*"
		]
	},
	"bundledDependencies": [
		"axios",
		"babel-plugin-dotenv",
