{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-proposal-class-properties",
		[
			"babel-plugin-styled-components",
			{
				"ssr": true,
				"displayName": true
			}
		],
		[
			"babel-plugin-dotenv",
			{
				"replacedModuleName": "babel-dotenv"
			}
		]
	]
}
